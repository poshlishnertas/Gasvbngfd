-- SAFE MOCK: Hacker Duplicator (Cosmetic only — fullscreen version, no close button)
-- Created for Jhon by ChatGPT (Mock Initialization, 0-100% over 3 minutes)

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local PlayerGui = player:WaitForChild("PlayerGui")

-- ScreenGui
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "Duplicator_Mock_Fullscreen"
ScreenGui.ResetOnSpawn = false
ScreenGui.IgnoreGuiInset = true
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.Parent = PlayerGui

-- Background (fullscreen)
local Background = Instance.new("Frame")
Background.Size = UDim2.new(1, 0, 1, 0)
Background.BackgroundColor3 = Color3.fromRGB(5, 5, 10)
Background.BorderSizePixel = 0
Background.Parent = ScreenGui

-- Hacker gradient effect
local Gradient = Instance.new("UIGradient")
Gradient.Rotation = 45
Gradient.Color = ColorSequence.new{
	ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 30, 0)),
	ColorSequenceKeypoint.new(0.5, Color3.fromRGB(0, 100, 0)),
	ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 40, 0))
}
Gradient.Parent = Background

-- Continuously rotate gradient
spawn(function()
	while ScreenGui.Parent do
		Gradient.Rotation = (Gradient.Rotation + 1) % 360
		RunService.RenderStepped:Wait()
	end
end)

-- Centered main frame
local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 400, 0, 220)
Frame.Position = UDim2.new(0.5, -200, 0.5, -110)
Frame.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
Frame.BorderSizePixel = 0
Frame.Parent = Background

local Corner = Instance.new("UICorner", Frame)
Corner.CornerRadius = UDim.new(0, 12)

local Stroke = Instance.new("UIStroke", Frame)
Stroke.Color = Color3.fromRGB(0, 255, 0)
Stroke.Thickness = 2
Stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border

-- Header
local Header = Instance.new("TextLabel")
Header.Size = UDim2.new(1, 0, 0, 40)
Header.BackgroundColor3 = Color3.fromRGB(0, 20, 0)
Header.Text = "☠ DUPLICATOR ☠"
Header.Font = Enum.Font.Code
Header.TextSize = 20
Header.TextColor3 = Color3.fromRGB(0, 255, 0)
Header.Parent = Frame

local HeaderCorner = Instance.new("UICorner", Header)
HeaderCorner.CornerRadius = UDim.new(0, 12)

-- Status Label
local Status = Instance.new("TextLabel")
Status.Size = UDim2.new(0.9, 0, 0, 36)
Status.Position = UDim2.new(0.05, 0, 0.55, 0)
Status.BackgroundTransparency = 1
Status.Text = "System Idle... Awaiting Input"
Status.Font = Enum.Font.Code
Status.TextSize = 14
Status.TextColor3 = Color3.fromRGB(0, 255, 0)
Status.TextWrapped = true
Status.Parent = Frame

-- Progress bar background
local ProgressBG = Instance.new("Frame")
ProgressBG.Size = UDim2.new(0.9, 0, 0, 20)
ProgressBG.Position = UDim2.new(0.05, 0, 0.34, 0)
ProgressBG.BackgroundColor3 = Color3.fromRGB(5, 5, 5)
ProgressBG.BorderSizePixel = 1
ProgressBG.Parent = Frame

local pCorner = Instance.new("UICorner", ProgressBG)
pCorner.CornerRadius = UDim.new(0, 6)

-- Progress fill
local ProgressFill = Instance.new("Frame")
ProgressFill.Size = UDim2.new(0, 0, 1, 0)
ProgressFill.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
ProgressFill.BorderSizePixel = 0
ProgressFill.Parent = ProgressBG

local fCorner = Instance.new("UICorner", ProgressFill)
fCorner.CornerRadius = UDim.new(0, 6)

-- Percent Label inside bar
local PercentLabel = Instance.new("TextLabel")
PercentLabel.Size = UDim2.new(1, 0, 1, 0)
PercentLabel.BackgroundTransparency = 1
PercentLabel.Text = "0%"
PercentLabel.Font = Enum.Font.Code
PercentLabel.TextScaled = true
PercentLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
PercentLabel.Parent = ProgressFill

-- Fade-in effect
Frame.BackgroundTransparency = 1
TweenService:Create(Frame, TweenInfo.new(0.6), { BackgroundTransparency = 0 }):Play()

-- Initialization sequence (mock): 0 → 100%
spawn(function()
	Status.Text = "INITIALIZING... 0%"
	local totalSeconds = 10
	local steps = 100
	local stepDuration = totalSeconds / steps

	for i = 1, steps do
		local percent = i
		local tween = TweenService:Create(ProgressFill, TweenInfo.new(stepDuration), {
			Size = UDim2.new(percent / 100, 0, 1, 0)
		})
		tween:Play()
		PercentLabel.Text = tostring(percent) .. "%"
		Status.Text = "INITIALIZING... " .. tostring(percent) .. "%"
		wait(stepDuration)
	end

	Status.Text = "SCRIPT LOADED!"
	ProgressFill.BackgroundColor3 = Color3.fromRGB(160, 0, 0)
	PercentLabel.Text = "100%"

	-- WAIT 2 SECONDS before fading away
	wait(2)

	-- Fade-out animation
	local fadeTime = 0.6
	local tweens = {}

	table.insert(tweens, TweenService:Create(Background, TweenInfo.new(fadeTime), { BackgroundTransparency = 1 }))
	table.insert(tweens, TweenService:Create(Frame, TweenInfo.new(fadeTime), { BackgroundTransparency = 1 }))
	table.insert(tweens, TweenService:Create(Header, TweenInfo.new(fadeTime), { TextTransparency = 1 }))
	table.insert(tweens, TweenService:Create(Status, TweenInfo.new(fadeTime), { TextTransparency = 1 }))
	table.insert(tweens, TweenService:Create(PercentLabel, TweenInfo.new(fadeTime), { TextTransparency = 1 }))
	table.insert(tweens, TweenService:Create(ProgressBG, TweenInfo.new(fadeTime), { BackgroundTransparency = 1 }))
	table.insert(tweens, TweenService:Create(ProgressFill, TweenInfo.new(fadeTime), { BackgroundTransparency = 1 }))

	for _, t in ipairs(tweens) do
		t:Play()
	end

	wait(fadeTime + 0.05)

	-- Destroy GUI
	ScreenGui:Destroy()

	-- === PLACE YOUR NEXT SCRIPT BELOW THIS LINE ===
	-- Example: print("Next script loading now...")
	-- ==============================================
end)

print("☠ Hacker Duplicator MOCK GUI Loaded (fullscreen cosmetic, no close button). ☠")
